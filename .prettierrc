{
  "trailingComma": "none",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "quoteProps": "preserve",
  "arrowParens": "always",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "strict",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "useTabs": false,
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "parser": "typescript"
      }
    }
  ],
  "importOrder": [
    "^@assets/(.*)$",
    "^@constants/(.*)$",
    "^@ui/(.*)$",
    "^@components/(.*)$",
    "^@pages/(.*)$",
    "^@reducers/(.*)$",
    "^@store/(.*)$",
    "^@services/(.*)$",
    "^@hooks/(.*)$",
    "^@utils/(.*)$",
    "^@customTypes/(.*)$",
    "^./*.*.ts",
    "^[./]",
    ".(scss|css)$"
  ],
  "importOrderSeparation": true
}
